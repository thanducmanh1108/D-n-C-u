<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div id="main">
       
        <div id="side-bar">
            <ul class="nav navbar-nav">
                <li class="none-li"><img src="~/Images/logo2.png"></li>
                <li><i class="fas fa-home"></i>@Html.ActionLink("Trang chủ", "About", "Home")</li>
                <li><i class="fas fa-tachometer-alt"></i>@Html.ActionLink("Dashboard", "Index", "Home")</li>
                <li><i class="fas fa-clipboard-check"></i>@Html.ActionLink("In Hóa Đơn", "Index", "banhangs")</li>
                <li><i class="fas fa-tv"></i>@Html.ActionLink("Sản phẩm", "Index", "sanphams")</li>
                <li><i class="fas fa-file-invoice"></i>@Html.ActionLink("Tổng Hóa đơn", "Index", "OrderDetails")</li>
                <li><i class="fas fa-hand-middle-finger"></i>@Html.ActionLink("Khách hàng", "Index", "kh_hang")</li> @*fas fa-user*@
                <li><i class="fas fa-suitcase"></i>@Html.ActionLink("Nhà cung cấp", "Index", "nccs")</li>
                <li><i class="fas fa-id-card"></i>@Html.ActionLink("Nhân viên", "Index", "nvs")</li>
                <li><i class="fas fa-list-ul"></i>@Html.ActionLink("Loại sản phẩm", "Index", "loais")</li>
                <li><i class="fas fa-warehouse"></i>@Html.ActionLink("Kho sản phẩm", "Index", "khohangs")</li>
                <li><i class="fas fa-dragon"></i>@Html.ActionLink("Hãng", "Index", "hangs")</li>

            </ul>
        </div>
        <div id="main-content">
            <div class="title" style=" padding-left: 10%;">
                <p>@ViewBag.Title</p>
            </div>
            <div style="height: 76px;"></div>
            @RenderBody()
        </div>

    </div>
    <footer>
        
    </footer>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="https://kit.fontawesome.com/a7f9139fa6.js" crossorigin="anonymous"></script>
</body>
</html>
