@model List<baithi.Models.OrderDetail>
@{
    ViewBag.Title = "Thanhtoan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            idsanpham
        </th>
        <th>
            tên sản phẩm
        </th>
        <th>
            soluongmua
        </th>

        <th>
            tongtien
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.idsanpham
            </td>
            <td>
                @item.tensp
            </td>
            <td>
                @item.soluongmua
            </td>
            <td>
                @item.tongtien
            </td>

        </tr>
    }

</table>


