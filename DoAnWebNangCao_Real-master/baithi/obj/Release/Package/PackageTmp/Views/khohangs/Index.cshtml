@model IEnumerable<baithi.Models.khohang>

@{
    ViewBag.Title = "Kho sản phẩm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<table class="table">
    <tr>
        <th>
            Tên kho
        </th>
        <th>
            Địa chỉ kho
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tenkho)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.diachi)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.IDkho }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.IDkho })
        </td>
    </tr>
}

</table>
<p class="create">
    @Html.ActionLink("Tạo kho sản phẩm mới", "Create")
</p>