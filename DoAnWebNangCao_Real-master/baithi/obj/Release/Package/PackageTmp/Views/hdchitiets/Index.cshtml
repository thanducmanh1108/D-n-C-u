
@{
    ViewBag.Title = "Hóa đơn";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var cardlist = (List<baithi.Models.CartItem>)ViewBag.Card;
}



<table class="table">
    <tr>
        <th>
            Mã sản phẩm
        </th>
        <th>
            Tên sản phẩm
        </th>

        <th>
            Số lượng
        </th>
        <th>
            Giá thành
        </th>
        <th>
            Thành tiền
        </th>
        <th>
            Mã khách hàng
        </th>
        <th>
            Mã nhân viên
        </th>

    </tr>

@foreach (var item in cardlist) {
<tr>
    <td>
        @item.sanpham.IDsp
    </td>
    <td>
         @item.sanpham.tensp
    </td>
    <td>
        @item.Quantity
    </td>
    <td>
        @item.sanpham.giathanh
    </td>
    <td>
        @item.sanpham.giathanh
    </td>
    <td>
        @item.sanpham.tensp
    </td>
    <td>
        @item.sanpham.tensp
    </td>

</tr>
}

</table>
